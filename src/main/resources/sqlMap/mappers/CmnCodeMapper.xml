<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ys.carInfo.common.mapper.CmnCodeMapper">

	<select id="selectCmnCdList" parameterType="HashMap" resultType="com.ys.carInfo.common.vo.CmnCodeVo">
		SELECT
				CMN_CD
			  , CMN_CD_NM
			  , NOTE
		FROM 	CMN_CODE
		WHERE	1 = 1
		AND 	USE_YN = 'Y'
		AND 	DEL_YN = 'N'
		ORDER	BY	SRT_ORD
				  , CMN_CD
	</select>


</mapper>